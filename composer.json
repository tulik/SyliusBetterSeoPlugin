{
    "name": "tulik/sylius-better-seo-plugin",
    "type": "sylius-plugin",
    "minimum-stability": "dev",
    "keywords": ["sylius", "sylius-plugin"],
    "description": "Support for SEO metadata on products",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "sylius/sylius": "dev-master"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behat/mink": "^1.8.x-dev",
        "behat/mink-browserkit-driver": "^1.3",
        "friends-of-behat/mink-extension": "^2.4",
        "behat/mink-selenium2-driver": "^v1.4.0",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "v1.4",
        "lakion/mink-debug-extension": "^v2.0",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-webmozart-assert": "^0.12",
        "phpunit/phpunit": "^9.4",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^4.0",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/intl": "^5.2",
        "symfony/web-profiler-bundle": "^4.4 || ^5.2",
        "symfony/web-server-bundle": "^4.0",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2"
    },
    "conflict": {
        "symfony/symfony": "4.1.8",
        "symfony/browser-kit": "4.1.8",
        "symfony/dependency-injection": "4.1.8",
        "symfony/dom-crawler": "4.1.8",
        "symfony/routing": "4.1.8"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "JoppeDc\\SyliusBetterSeoPlugin\\": "src/",
            "Tests\\JoppeDc\\SyliusBetterSeoPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
